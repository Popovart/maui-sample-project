<?xml version="1.0" encoding="utf-8"?>
<views:BaseContentPage
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:viewModels="clr-namespace:MauiApp1.Presentation.ViewModels"
	xmlns:views="clr-namespace:MauiApp1.Presentation.Views"
	x:DataType="viewModels:DetailViewModel"
	x:Class="MauiApp1.Presentation.Views.DetailPage"
>
	<ContentPage.Content>
		<StackLayout>
			<Label
				Text="{Binding TaskData.Name}"
				FontSize="25"
				VerticalOptions="Center"
				HorizontalOptions="Center"
			/>
			<!-- <Button Text="Go Back" Command="{Binding GoBackCommand}" /> -->
		</StackLayout>
	</ContentPage.Content>
</views:BaseContentPage>
