<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodel="clr-namespace:MauiApp1.ViewModels"
             x:DataType="viewmodel:DetailViewModel"
             x:Class="MauiApp1.Views.DetailPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Task.Name}" FontSize="25" VerticalOptions="Center" HorizontalOptions="Center" />
            <!-- <Button Text="Go Back" Command="{Binding GoBackCommand}" /> -->
        </StackLayout>
    </ContentPage.Content>
</ContentPage>